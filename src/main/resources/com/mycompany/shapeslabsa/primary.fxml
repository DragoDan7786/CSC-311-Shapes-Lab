<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.shapeslabsa.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#moveRect" prefHeight="33.0" prefWidth="108.0" text="Rect Right 100" />
         </children>
      </HBox>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Circle fill="DODGERBLUE" layoutX="52.0" layoutY="200.0" radius="52.0" stroke="BLACK" strokeType="INSIDE" />
            <Rectangle fx:id="theRectangle" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="104.0" layoutX="130.0" layoutY="148.0" stroke="BLACK" strokeType="INSIDE" width="120.0" />
         </children>
      </Pane>
   </children>
</VBox>
